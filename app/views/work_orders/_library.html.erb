<%= form.fields_for :library, Library.new do |library| %>
  <div class="form-group" >
    <%= library.label :volume %>
    <%= library.text_field :volume %>
  </div>
  <div class="form-group" >
    <%= library.label :kit_number %>
    <%= library.text_field :kit_number %>
  </div>
  <div class="form-group" >
    <%= library.label :ligase_batch_number %>
    <%= library.text_field :ligase_batch_number %>
  </div>
<% end %>