<%= form_for work_order do |f| %>
  <fieldset>
    <legend>
      Work Order
    </legend>
    <div><%= work_order.sample_name %></div>
    <%= render work_order.template, form: f %>
    <div class="form-group">
      <%= f.submit class: 'btn btn-primary pull-right' %>
    </div>
  </fieldset>
<% end %>